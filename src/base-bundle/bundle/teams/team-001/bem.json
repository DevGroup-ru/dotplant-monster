{
  "container": true,
  "content": {
    "block": "team-001",
    "row": true,
    "utils": [
      "one-line--top-bottom"
    ],
    "content": [
      {
        "elem": "title",
        "utils": [
          "no-margin--top"
        ],
        "editable": {
          "type": "text",
          "key": "title"
        }
      },
      {
        "recursive": "blocks",
        "itemTemplate": {
          "elem": "nested",
          "content": [
            {
              "elem": "photo-link",
              "editable": {
                "type": "link",
                "key": "photo-link",
                "target": "item"
              },
              "content": [
                {
                  "elem": "photo",
                  "editable": {
                    "type": "image",
                    "key": "img",
                    "target": "item"
                  }
                }
              ]
            },
            {
              "elem": "name",
              "editable": {
                "type": "link",
                "key": "name",
                "target": "item"
              }
            },
            {
              "elem": "position",
              "editable": {
                "key": "position",
                "target": "item"
              }
            },
            {
              "elem": "text",
              "editable": {
                "type": "wysiwyg",
                "key": "text",
                "target": "item"
              }
            },
            {
              "elem": "hours-wrap",
              "content": [
                {
                  "elem": "title-hours",
                  "editable": {
                    "key": "title-hours",
                    "target": "item"
                  }
                },
                {
                  "elem": "hours",
                  "html": "пн 10:00 - 15:00"
                },
                {
                  "elem": "hours",
                  "html": "вт 10:00 - 15:00"
                },
                {
                  "elem": "hours",
                  "html": "ср 10:00 - 15:00"
                },
                {
                  "elem": "hours",
                  "html": "чт 10:00 - 15:00"
                },
                {
                  "elem": "hours",
                  "html": "пт 10:00 - 15:00"
                },
                {
                  "elem": "hours",
                  "html": "сб-вс выходной"
                }
              ]
            },
            {
              "elem": "phone",
              "editable": {
                "key": "phone",
                "target": "item"
              }
            },
            {
              "elem": "mail",
              "editable": {
                "key": "mail",
                "target": "item"
              }
            },
            {
              "elem": "social-wrap",
              "content": [
                {
                  "elem": "social-item",
                  "html": "<a href='/' class='team-001__social-icon'>f</a>"
                },
                {
                  "elem": "social-item",
                  "html": "<a href='/' class='team-001__social-icon'>v</a>"
                },
                {
                  "elem": "social-item",
                  "html": "<a href='/' class='team-001__social-icon'>t</a>"
                }
              ]
            },
            {
              "elem": "nested-link-wrap",
              "content": [
                {
                  "elem": "nested-link",
                  "cls": "button-simple",
                  "editable": {
                    "type": "link",
                    "key": "nested-link",
                    "target": "item"
                  }
                }
              ]
            }
          ]
        }
      },
      {
        "elem": "btn-wrap",
        "content": [
          {
            "elem": "full-link",
            "cls": "button-simple",
            "editable": {
              "type": "link",
              "key": "full-link"
            }
          }
        ]
      }
    ]
  }
}
